{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./tsconfig.frontend.json",
  "compilerOptions": {
    "noEmit": false,
    "declaration": true,
    "emitDeclarationOnly": true,
    "declarationDir": "build/types/frontend",
    "outDir": "build/types/frontend",
    "allowJs": false
  },
  "include": [
    "frontend/**/*.ts"
  ]
}